{
	"name": "wegate",
	"dockerFile": "Dockerfile",
	"extensions": [
		"ms-vscode.go",
		"yzhang.markdown-all-in-one",
		"davidanson.vscode-markdownlint",
		"wmaurer.change-case",
		"asuka.insertnumbers"
	],
	"runArgs": [
		"--cap-add=SYS_PTRACE",
		"--security-opt",
		"seccomp=unconfined"
	],
	"settings": {
		"go.gopath": "/go",
		"go.useLanguageServer": true
	}
}
